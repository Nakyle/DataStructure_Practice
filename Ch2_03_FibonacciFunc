public class _03_FibonacciFunc {
	public static void main(String[] args) {
		int i;

		for (i = 1; i < 15; i++) {
			System.out.print(Fibo(i) + " ");
		}

		// Fibo(7);
	}

	public static int Fibo(int n) {
		// System.out.println("func call param " + n);

		if (n == 1)
			return 0;
		else if (n == 2)
			return 1;
		else
			return Fibo(n - 1) + Fibo(n - 2);
	}
}
